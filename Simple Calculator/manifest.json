{
    "name": "Simple Calculator",
    "version": "0.2.1",
    "author": "TeakIvy",
    "description": "A Simple calculator for any Chrome/Chromium browser!",
    "manifest_version": 2,
    "icons": { 
        "16": "icon16.png",
        "32": "icon32.png",
        "48": "icon48.png",
        "128": "icon128.png" },
    "offline_enabled": true,
    "browser_action": {
        "default_icon": "icon.png",
        "default_popup": "popup.html",
        "default_title": "Simple Calculator"
    }
}